{
  "name": "vue-minimal",
  "version": "1.0.0",
  "main": "index.js",
  "homepage": "https://github.com/dimpadev/vue-minimal",
  "repository": "git@github.com:dimpadev/vue-minimal.git",
  "author": "Dimitri Pandeleakis <dimitris88@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --hot",
    "build.dev.modern": "cross-env _BABEL_MODE=modern NODE_ENV=development webpack --mode=development --config ./build/webpack.config.js",
    "build.dev.legacy": "cross-env _BABEL_MODE=legacy NODE_ENV=development webpack --mode=development --config ./build/webpack.config.js",
    "build.prod.modern": "cross-env _BABEL_MODE=modern NODE_ENV=production webpack --mode=production --config ./build/webpack.config.js",
    "build.prod.legacy": "cross-env _BABEL_MODE=legacy NODE_ENV=production webpack --mode=production --config ./build/webpack.config.js",
    "nwjs.run.nw1": "nw --ignore-gpu-blacklist --disable-features=nw2 build/dist",
    "nwjs.run.nw2": "nw --ignore-gpu-blacklist --enable-features=nw2 build/dist"
  },
  "dependencies": {
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.1.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "node-sass-glob-importer": "^5.3.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-safe-parser": "^4.0.1",
    "postcss-url": "^8.0.0",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.2",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "style-loader": "^1.1.2",
    "terser-webpack-plugin": "^2.3.2",
    "vue-loader": "^15.8.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-notifier": "^1.8.0"
  }
}
